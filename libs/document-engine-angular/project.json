{
  "name": "@phuong-tran-redoc/document-engine-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/document-engine-angular/src",
  "projectType": "library",
  "release": {
    "version": {
      "manifestRootsToUpdate": ["dist/{projectRoot}"],
      "currentVersionResolver": "git-tag",
      "fallbackCurrentVersionResolver": "disk"
    }
  },
  "tags": ["scope:public"],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/libs/document-engine-angular"],
      "options": {
        "project": "libs/document-engine-angular/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/document-engine-angular/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/document-engine-angular/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/document-engine-angular/jest.config.ts"
      }
    }
  }
}
